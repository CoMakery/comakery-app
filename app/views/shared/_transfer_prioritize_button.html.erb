<%= turbo_frame_tag "transfer_prioritize_button_#{transfer.id}", target: '_top' do %>
  <% if transfer.decorate.show_prioritize_button? %>
    <div class="transfers-table__transfer__button__history">
      <%= link_to project_dashboard_transfer_prioritizes_path(project_id: transfer.project.id, transfer_id: transfer.id),
                  class: 'transfer-algo-btn',
                  rel: 'nofollow',
                  method: :patch,
                  id: 'prioritizeBtn',
                  data: { turbo: 'false' } do %>
        <%= render partial: 'shared/transfer_prioritize_button/mode', locals: { project: transfer.project } %>
      <% end %>
    </div>
  <% end %>
<% end %>
